urxvt*font: xft:SpaceMono Nerd Font Mono:size=15:antialias=true:hinting=true, \
            xft:Symbola:pixelsize=14:antialias=true:hinting=slight

urxvt*letterSpace: 0
urxvt*lineSpace: -1

urxvt*scrollBar: false
urxvt.saveLines: 9999
urxvt.iso14755: false
urxvt*transparent: false
urxvt*internalBorder: 15
urxvt*borderLess:false

URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher,config-reload

#include ".xcolors"
#if __has_include(".xhost")
#include ".xhost"
#endif
